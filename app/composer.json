{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "*",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.12",
    "doctrine/doctrine-migrations-bundle": "^3.3",
    "doctrine/orm": "^3.2",
    "easycorp/easyadmin-bundle": "*",
    "endroid/qr-code-bundle": "*",
    "karser/karser-recaptcha3-bundle": "^0.1.27",
    "knplabs/knp-paginator-bundle": "*",
    "league/csv": "^9.16.0",
    "lexik/jwt-authentication-bundle": "^3.1",
    "liip/imagine-bundle": "^2.12",
    "mailjet/mailjet-apiv3-php": "*",
    "nelmio/cors-bundle": "^2.5",
    "nucleos/dompdf-bundle": "^4.3",
    "phpdocumentor/reflection-docblock": "^5.4",
    "phpstan/phpdoc-parser": "^1.29",
    "symfony/asset": "^6.4|^7.3",
    "symfony/asset-mapper": "^6.4|^7.3",
    "symfony/console": "*",
    "symfony/doctrine-messenger": "*",
    "symfony/dotenv": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "*",
    "symfony/form": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/intl": "*",
    "symfony/mailer": "*",
    "symfony/mailgun-mailer": "^6.4|^7.3",
    "symfony/mailjet-mailer": "^6.4|^7.3",
    "symfony/mime": "*",
    "symfony/monolog-bundle": "*",
    "symfony/notifier": "*",
    "symfony/process": "*",
    "symfony/property-access": "*",
    "symfony/property-info": "*",
    "symfony/runtime": "*",
    "symfony/security-bundle": "^7.1",
    "symfony/serializer": "*",
    "symfony/string": "*",
    "symfony/translation": "*",
    "symfony/twig-bundle": "*",
    "symfony/validator": "*",
    "symfony/web-link": "*",
    "symfony/yaml": "*",
    "symfonycasts/reset-password-bundle": "^1.23",
    "symfonycasts/verify-email-bundle": "*",
    "tales-from-a-dev/flowbite-bundle": "*",
    "twig/cssinliner-extra": "^3.10",
    "twig/extra-bundle": "*",
    "twig/intl-extra": "*",
    "twig/string-extra": "^3.21",
    "twig/twig": "*",
    "vich/uploader-bundle": "*"
  },
  "require-dev": {
    "dbrekelmans/bdi": "^1.0",
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "*",
    "symfony/css-selector": "*",
    "symfony/debug-bundle": "*",
    "symfony/maker-bundle": "^1.64",
    "symfony/phpunit-bridge": "*",
    "symfony/stopwatch": "*",
    "symfony/web-profiler-bundle": "*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true,
      "endroid/installer": true,
      "composer/installers": true,
      "oomphinc/composer-installers-extender": true,
      "robloach/component-installer": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd",
      "importmap:install": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  }
}
