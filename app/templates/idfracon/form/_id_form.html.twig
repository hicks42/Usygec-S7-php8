<div class="w-full">
		{{ form_start(form) }}
		<div class="flex flex-wrap w-full">
			{{ form_row(form.lastName) }}
			{{ form_row(form.firstName) }}
		</div>
		<div class="flex flex-wrap w-full">
			{{ form_row(form.birthDate) }}
			{{ form_row(form.birthPlace) }}
		</div>
		<div class="flex flex-wrap w-full">
			{{ form_row(form.address1) }}
			{{ form_row(form.address2) }}
		</div>
		<div class="flex flex-wrap w-full">
			{{ form_row(form.cp) }}
			{{ form_row(form.city) }}
		</div>
		<div class="flex flex-wrap w-full">
			{{ form_row(form.country) }}
		</div>
		{{ form_end(form) }}
</div>
